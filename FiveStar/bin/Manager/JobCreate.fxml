<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Manager.JobCreateController">
   <center>
      <BorderPane onMouseEntered="#refresh" prefHeight="400.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <bottom>
            <HBox alignment="CENTER" prefHeight="65.0" prefWidth="560.0" spacing="30.0" translateX="-20.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
                  <Button mnemonicParsing="false" onAction="#enter" text="Enter" />
               </children>
            </HBox>
         </bottom>
         <center>
            <GridPane hgap="10.0" maxWidth="550.0" prefHeight="310.0" prefWidth="550.0" vgap="10.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <ComboBox fx:id="adSelector" prefWidth="150.0" promptText="Advertiser/Ref" GridPane.rowIndex="1" />
                  <ComboBox fx:id="crewSelector" prefWidth="150.0" promptText="Crew" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="quote" promptText="Quote" GridPane.rowIndex="3" />
                  <TextField fx:id="footage" promptText="Footage" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <CheckBox alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Repair" GridPane.columnIndex="3" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
                  <DatePicker fx:id="date" promptText="Date" GridPane.columnIndex="3" GridPane.rowIndex="4" />
                  <TextField fx:id="street" promptText="Street" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <TextField fx:id="city" promptText="City" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                  <TextField fx:id="state" promptText="State" GridPane.rowIndex="4" />
                  <TextField fx:id="zipCode" promptText="Zip Code" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <TextField fx:id="laborCost" promptText="Labor Cost" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                  <ComboBox fx:id="customerSelector" prefWidth="150.0" promptText="Customer" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <TextField fx:id="jobName" promptText="Job Name" GridPane.rowIndex="5" />
               </children>
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
            </GridPane>
         </center>
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem fx:id="ref" mnemonicParsing="false" onAction="#refresh" text="Refresh" />
                    <MenuItem mnemonicParsing="false" onAction="#cancel" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <right>
            <BorderPane prefHeight="200.0" prefWidth="250.0" BorderPane.alignment="CENTER">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <padding>
                  <Insets right="15.0" top="5.0" />
               </padding>
               <center>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Select Materials" />
                        <ListView fx:id="materialSelector" prefHeight="200.0" prefWidth="200.0" />
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#selectAmount" text="Add" />
                              <Button mnemonicParsing="false" onAction="#remove" text="Remove" />
                           </children>
                           <padding>
                              <Insets left="10.0" right="10.0" />
                           </padding>
                        </HBox>
                        <Label text="Selected Materials" />
                        <ListView fx:id="materialSelected" prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </center>
               <right>
                  <GridPane prefHeight="305.0" prefWidth="53.0" BorderPane.alignment="CENTER">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </right>
            </BorderPane>
         </right>
      </BorderPane>
   </center>
</BorderPane>
