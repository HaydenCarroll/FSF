<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane onMouseEntered="#initalize" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Manager.FSF">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#print" text="Save" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <ScrollPane prefHeight="2200.0" prefWidth="1700.0" BorderPane.alignment="CENTER">
        <content>
          <AnchorPane fx:id="pane" minHeight="0.0" minWidth="0.0" prefHeight="2200.0" prefWidth="1700.0">
               <children>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@Quote.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="date" layoutX="181.0" layoutY="375.0" prefHeight="30.0" prefWidth="274.0" text="Date">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
                  <Label fx:id="name" layoutX="576.0" layoutY="375.0" prefHeight="38.0" prefWidth="478.0" text="Name">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
                  <Label fx:id="address" layoutX="210.0" layoutY="432.0" prefHeight="38.0" prefWidth="839.0" text="Address">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
                  <Label fx:id="phoneNum" layoutX="241.0" layoutY="489.0" prefHeight="38.0" prefWidth="295.0" text="Phone Number">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
                  <Label fx:id="ref" layoutX="715.0" layoutY="489.0" prefHeight="38.0" prefWidth="346.0" text="Refrence">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
                  <Label fx:id="email" layoutX="181.0" layoutY="546.0" prefHeight="38.0" prefWidth="839.0" text="Email">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
                  <Label fx:id="jobName" layoutX="375.0" layoutY="602.0" prefHeight="38.0" prefWidth="680.0" text="Job Name">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </center>
</BorderPane>
